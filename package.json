{
  "name": "food",
  "version": "1.0.0",
  "description": "Application to manage food stuff",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "clean": "npx rimraf dist",
    "build": "webpack --config webpack.config.js",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer ./stats.json",
    "format": "prettier --check \"**/*.*\"",
    "lint": "eslint . --ext .tsx,.ts,.js webpack.config.*",
    "lint:report": "eslint . --ext .tsx .ts .js webpack.config.* --output-file ./report/eslint-report.html --format html",
    "test": "jest",
    "pre-push": "npm run lint && npm run format && npm run test && npm run clean && npm run build"
  },
  "author": "nathan m weller",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-router-dom": "6.3.0",
    "vanilla-toast": "0.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.5",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "2.4.5",
    "@types/webpack": "5.28.0",
    "@types/webpack-dev-server": "4.7.1",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "babel-loader": "8.2.4",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "eslint": "8.12.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-testing-library": "^5.2.1",
    "file-loader": "6.2.0",
    "jest": "27.5.1",
    "jest-axe": "^6.0.0",
    "msw": "^0.39.2",
    "prettier": "2.6.2",
    "react-refresh": "0.12.0",
    "redux-immutable-state-invariant": "2.1.0",
    "style-loader": "3.3.1",
    "type-fest": "2.12.2",
    "typescript": "4.6.3",
    "url-loader": "4.1.1",
    "webpack": "5.72.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1",
    "whatwg-fetch": "^3.6.2"
  }
}
